<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.*?>
<?scenebuilder-background-color 0xc5c5c5ff?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="application.StartUpMenuController">
  <center>
    <Button fx:id="intermediateButton" mnemonicParsing="false" onAction="#clickedIntermediateButton" text="Intermediate" />
  </center>
  <left>
    <Button fx:id="noviceButton" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#clickedNoviceButton" text="Novice" textAlignment="CENTER" />
  </left>
  <right>
    <Button fx:id="advancedButton" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#clickedAdvancedButton" text="Advanced" textAlignment="CENTER" textOverrun="CLIP" />
  </right>
  <top>
    <Label alignment="CENTER" prefWidth="642.0" text="MINESWEEPER" textAlignment="CENTER" textFill="BLACK" BorderPane.alignment="TOP_CENTER">
      <font>
        <Font name="Segoe UI Black" size="80.0" />
      </font>
    </Label>
  </top>
</BorderPane>
